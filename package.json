{
  "name": "node-red-vue",
  "version": "0.1.12",
  "description": "Write Node-RED node templates using Vue.js",
  "keywords": [
    "vue",
    "vue.js",
    "node-red"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tve/node-red-vue.git"
  },
  "author": "Thorsten von Eicken",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "*.js",
    "*.html",
    "*.md",
    "resources/**/*"
  ],
  "dependencies": {
    "@vue/compiler-sfc": "^3.2.0",
    "hash-sum": "^2.0.0",
    "mitt": "^3.0.0"
  },
  "node-red": {
    "version": ">=3.0.0",
    "plugins": {
      "node-red-vue": "node-red-vue.js"
    }
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
